prefix=${pcfiledir}/../..
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include

Name: absl_if_constexpr
Description: Abseil if_constexpr library
URL: https://abseil.io/
Version: 20250127

Libs: "-L${libdir}"
Requires: absl_config = 20250127
Cflags: "-I${includedir}" -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
